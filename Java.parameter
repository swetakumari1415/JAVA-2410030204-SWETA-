class Calculator {
    // method with 2 parameters
    int add(int a, int b) {
        return a + b;
    }
    // overloaded method with 3 parameters
    int add(int a, int b, int c) {
        return a + b + c;
    }
}
public class Main {
    public static void main(String[] args) {
        Calculator calc = new Calculator();
        System.out.println(calc.add(5, 10));       // calls 2-param method
        System.out.println(calc.add(5, 10, 15));   // calls 3-param method
    }
}
