public class Geeks {
// static variable
static int a = 40;
// instance variable
int b = 50void simpleDisplay()
1
{
System.out.println(a);
System.out.println(b);
}
// Declaration of a static method
static void staticDisplay()
{
}
System.out.println(a);
// main method
public static void main(String[] args)
{
Geeks obj = new Geeks();
obj.simpleDisplay();
// Calling static method
Geeks.staticDisplay();
staticDisplay();
}
}

